<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>Đăng ký tài khoản</title>

	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false; function hideURLbar(){ window.scrollTo(0,1); } </script>
	<link rel="stylesheet" href="content/css/styleDangKy.css" type="text/css" media="all" />
	<link rel="stylesheet" href="content/font-awesome/css/font-awesome.css">
</head>
<body>

<div class="center-container">
	
	<div class="header-w3l">
		<h1>Trắc nghiệm trực tuyến</h1>
	</div>
	
	<div class="main-content-agile">
		<div class="sub-main-w3">	
			<div class="wthree-pro">
				<h2>Đăng ký</h2>
			</div>
			<form action="#" method="post">
				<!-- Tên đăng nhập -->
				<div class="pom-agile">
					<input placeholder="*Email" name="Name" class="user" type="email" required="">
					<span class="icon1"><i class="fa fa-user" aria-hidden="true"></i></span>
				</div>
				
				<div class="pom-agile">
					<input  placeholder="*Mật khẩu" name="Password" class="Infor" type="password" required="">
					<span class="icon2"><i class="fa fa-unlock" aria-hidden="true"></i></span>
				</div>
				
				<div class="pom-agile">
					<input  placeholder="*Xác nhận mật khẩu" name="RePassword" class="Infor" type="password" required="">
					<span class="icon2"><i class="fa fa-unlock" aria-hidden="true"></i></span>
				</div>
			
			
				<div class="pom-agile">
					<input  placeholder="*Họ và tên" name="Fullname" class="Infor" type="text" required="">
					<span class="icon2"><i class="fa fa-user" aria-hidden="true"></i></span>
				</div>
			
				<div class="pom-agile">
					<input  placeholder="Số điện thoại" name="Tele-num" class="Infor" type="text" required="">
					<span class="icon2"><i class="fa fa-user" aria-hidden="true"></i></span>
				</div>
				
				<div class="pom-agile">
					<input  placeholder="*Địa chỉ" name="Address" class="Infor" type="text" required="">
					<span class="icon2"><i class="fa fa-user" aria-hidden="true"></i></span>
				</div>
			
				<div class="sub-w3l">
					<div class="right-w3l">
						<input type="submit" value="Đăng ký">
					</div>
				</div>
				

			</form>
		</div>
	</div>
	

	<div class="footer">
		<p> Copyright &copy; My Website 2018</p>
	</div>
	
</div>
</body>
</html>